{
  "name": "nodejs-project-1",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write \"**/*.ts\"",
    "build": "tsc",
    "protos": "pbjs -t static-module -w commonjs -o src/protos/protos.js src/protos/*.proto && pbts -o src/protos/protos.d.ts src/protos/protos.js"
  },
  "author": "Inga Adejanova",
  "license": "MIT",
  "dependencies": {
    "@grpc/proto-loader": "^0.7.10",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^20.11.25",
    "fastify": "^4.26.2",
    "grpc": "^1.24.11",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "grpc-tools": "^1.0.0",
    "nodemon": "^3.1.0",
    "pbjs": "^0.0.14",
    "pbts": "^4.0.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2"
  }
}
